alias ll='ls -la' 
alias vim='nvim'
alias h='history'
alias grep='grep --color=auto'
alias sp='sudo pacman'
alias glog='git log --graph --oneline --decorate'
alias wifi-connect='nmcli --ask device wifi connect'
alias wifi-show='nmcli device wifi list'
alias config='/usr/bin/git --git-dir=$HOME/dev/github/configs --work-tree=$HOME'
alias lg='lazygit'
alias maim-png-to-clipboard='maim -s | xclip -selection clipboard -t image/png'
alias screencap='maim -s | xclip -selection clipboard -t image/png'
